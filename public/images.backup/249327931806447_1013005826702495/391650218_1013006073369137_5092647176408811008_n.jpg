{
  "manifest_version": 3,
  "name": "SnapNote",
  "version": "0.0.2",
  "description": "SnapNote is an open-source note-taking extension created with the goals of transparency, convenience, and most importantly, speed.",
  "permissions": ["clipboardWrite", "downloads"],

  "icons": {
    "48": "icon.png"
  },

  "action": {
    "default_popup": "index.html"
  }
}
